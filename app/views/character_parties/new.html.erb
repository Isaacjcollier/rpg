<h1> Character Parties </h1>

<%= form_for CharacterParty.new do |f| %>

    <%= f.label :character_id %>
    <%= f.collection_select :character_id, Character.all, :id, :name %>

    <%= f.label :party_id %>
    <%= f.collection_select :party_id, Party.all, :id, :name %>

    <%= f.submit %>

  <%  end %>
